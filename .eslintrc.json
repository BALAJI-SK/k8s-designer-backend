{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "globals": {
        "process": true,
        "__dirname": true,
        "describe": true,
        "it": true,
        "jest":true,
        "expect":true,
        "before": true,
        "after": true,
        "beforeEach": true,
        "afterEach": true,
        "beforeAll":true,
        "afterAll":true

      },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
